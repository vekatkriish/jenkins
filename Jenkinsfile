pipeline {
    agent { docker { image 'maven:3.3.3' }, docker {image 'node:6.3'} }
    stages {
	    
        stage('build') {
            steps {
                sh 'npm --version'
		sh 'npm --version'
                sh 'mvn --version'
		sh "echo venkatesh"
                sh "echo jagathpathi"
            }
        }
    }
}

